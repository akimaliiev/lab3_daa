
#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double K;
    double N;
    double result = 0;

    cout << "Enter N: ";
    cin >> N;

    cout << "Enter K: ";
    cin >> K;

    for (int i = 1; i <= N; i++)
    {
        result += pow(i, K);
    }

    cout << "Result = " << result << endl;
}
