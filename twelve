#include <iostream>

using namespace std;

int main()
{
    double a0 = 1;
    int N;

    cout << "Enter N(> 0): ";
    cin >> N;

    double result;
    double temp = a0;
    // cout << a0 << ", ";

    for (int i = 1; i <= N; i++)
    {
        result = (temp + 1) / i;
        cout << result << ", ";
        temp = result;
    }
    cout << endl;
}
