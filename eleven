#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    double a0 = 2;
    int N;

    cout << "Enter N(> 0): ";
    cin >> N;

    double result;
    double temp = a0;
    // cout << a0 << ", ";

    for (int i = 1; i <= N; i++)
    {
        result = 2 + (1 / temp);
        cout << result << ", ";
        temp = result;
    }
    cout << endl;
}
